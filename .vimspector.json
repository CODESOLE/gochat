{
  "configurations": {
    "Launch Server with args: 1234": {
      "adapter": "delve",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/cmd/server/server.go",
        "cwd": "${workspaceRoot}",
        "env": {
          "BASEPATH": "${workspaceRoot}"
        },
        "args": [
          "1234"
        ]
      }
    },
    "Launch Client with args: localhost:1234": {
      "adapter": "delve",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/cmd/client/client.go",
        "cwd": "${workspaceRoot}",
        "env": {
          "BASEPATH": "${workspaceRoot}"
        },
        "args": [
          "localhost:1234"
        ]
      }
    }
  }
}
